<div class="container">
      <div class="container-child col-md-4 col-sm-6 col-xs-8 ">
        <table class="followers">
        <% @user.followed_users.each do |followed_user| %>
          <tr>
            <td><%= profile_img(followed_user) %></td>
            <td><%= followed_user.name %></td>
            <td><%= render 'follow_form', user: followed_user %></td>
          </tr>

        <% end %>
        </table>
      </div>
</div>
