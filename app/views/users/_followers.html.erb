<div class="container">
      <div class="container-child col-md-4 col-sm-6 col-xs-8 ">
        <table class="followers">
        <% @user.followers.each do |follower| %>
          <tr>
            <td><%= profile_img(follower) %></td>
            <td><%= follower.name %></td>
            <td><%= render 'follow_form', user: follower %></td>
          </tr>
          <% end %>
        </table>
      </div>
</div>
